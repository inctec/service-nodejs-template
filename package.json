    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-immutable": "^6.0.1"
  },
  "dependencies": {
    "@types/config": "0.0.34",
    "@types/dotenv": "^6.1.1",
    "@types/ioredis": "^4.0.17",
    "@types/node": "^12.7.5",
    "@types/ramda": "^0.26.21",
    "@types/wait-on": "^3.2.0",
